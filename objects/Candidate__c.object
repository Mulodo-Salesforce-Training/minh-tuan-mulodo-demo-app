SUBSTITUTE(Phone__c, 
&quot;(&quot;, &quot;&quot;), &quot;)&quot;, &quot;&quot;), &quot; &quot;, &quot;&quot;), &quot;-&quot;, &quot;&quot;) , 
&quot;(\\d){10}&quot;)
),
NOT(REGEX( 
